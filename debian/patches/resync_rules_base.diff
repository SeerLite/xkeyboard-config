# To rebuild this patch, run
  ./debian/rules config.status
  cd rules/compat
  quilt add base.l[2-4]*.part base.m*.part
  rm -f base.l[2-4]*.part base.m*.part
  make
  cd ..
  quilt add base base.lst
  rm -f base base.lst
  make base base.lst
  cd ..
  quilt refresh --sort
  exit 0

